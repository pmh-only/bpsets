    "@yao-pkg/pkg": "^6.2.0",
    "nodemon": "^3.1.9",
    "typescript": "^5.7.2"
  },
  "pkg": {
    "scripts": "build/**/*",
    "assets": [
      "views/**/*",
      "bpset_metadata.json"
    ],
    "targets": [
      "node22-linuxstatic-x86_64",
      "node22-linuxstatic-arm64"
