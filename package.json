{
  "name": "bpsets",
  "version": "0.1.0",
  "main": "build/main.js",
  "bin": "build/main.js",
  "scripts": {
    "pkg": "rimraf ./build && tsc && pkg .",
    "build": "rimraf ./build && tsc",
    "start": "node .",
    "dev": "nodemon -e ts --exec \"rimraf ./build && tsc && node . || exit 1\""
