    "@aws-sdk/client-securityhub": "^3.716.0",
    "@aws-sdk/client-sns": "^3.716.0",
    "@aws-sdk/client-ssm": "^3.716.0",
    "@aws-sdk/client-sts": "^3.716.0",
    "@aws-sdk/client-wafv2": "^3.716.0",
